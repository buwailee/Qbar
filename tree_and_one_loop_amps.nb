  3.52785008596115*^9}},ExpressionUUID->"5619528b-3aa2-40b3-ba72-\
f672de51c58b"],

Cell[BoxData[{
 RowBox[{"ClearAll", "[", "Atree", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Atree", "[", 
    RowBox[{"0", ",", "Zs_"}], "]"}], "=", "1"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Atree", "[", 
   RowBox[{"k_", ",", "n_Integer"}], "]"}], ":=", 
  RowBox[{"Atree", "[", 
   RowBox[{"k", ",", 
    RowBox[{"IdentityMatrix", "[", "n", "]"}]}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{"Atree", "[", 
   RowBox[{"k_", ",", "Zs_"}], "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"Length", "[", "Zs", "]"}], "<", 
      RowBox[{"4", "+", "k"}]}], "||", 
     RowBox[{"k", "<", "0"}]}], ",", "0", ",", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Length", "[", "Zs", "]"}], "\[Equal]", "5"}], ",", 
      "\[IndentingNewLine]", " ", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"the", " ", "basic", " ", "R"}], "-", "invariant"}], " ", 
       "*)"}], "\[IndentingNewLine]", " ", 
      RowBox[{"Block", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"fourbrackets", "=", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"fourbracket", "@", 
             RowBox[{
              RowBox[{"RotateLeft", "[", 
               RowBox[{"Zs", ",", "l"}], "]"}], "[", 
              RowBox[{"[", 
               RowBox[{"1", ";;", "4"}], "]"}], "]"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"l", ",", "1", ",", "5"}], "}"}]}], "]"}]}], "}"}], ",", 
        "\[IndentingNewLine]", " ", 
        RowBox[{
         RowBox[{"Dmatrix", "[", 
          RowBox[{"{", 
           RowBox[{"Sum", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Zs", "[", 
               RowBox[{"[", "l", "]"}], "]"}], 
              RowBox[{"fourbrackets", "[", 
               RowBox[{"[", "l", "]"}], "]"}]}], ",", 
             RowBox[{"{", 
              RowBox[{"l", ",", "1", ",", "5"}], "}"}]}], "]"}], "}"}], "]"}],
          "/", 
         RowBox[{"Product", "[", 
          RowBox[{
           RowBox[{"fourbrackets", "[", 
            RowBox[{"[", "l", "]"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"l", ",", "1", ",", "5"}], "}"}]}], "]"}]}]}], "]"}], ",",
       "\[IndentingNewLine]", " ", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"n", ">", "5"}], ":", " ", 
        RowBox[{"apply", " ", "the", " ", "recursion", " ", "relation"}]}], 
       " ", "*)"}], "\[IndentingNewLine]", " ", 
      RowBox[{
       RowBox[{"Atree", "[", 
        RowBox[{"k", ",", 
         RowBox[{"Drop", "[", 
          RowBox[{"Zs", ",", 
           RowBox[{"-", "1"}]}], "]"}]}], "]"}], "+", 
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{"Block", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"Zmm", "=", 
             RowBox[{"inter", "[", 
              RowBox[{"Zs", "[", 
               RowBox[{"[", 
                RowBox[{"{", 
                 RowBox[{"m", ",", 
                  RowBox[{"m", "+", "1"}], ",", 
                  RowBox[{"-", "2"}], ",", 
                  RowBox[{"-", "1"}], ",", "1"}], "}"}], "]"}], "]"}], 
              "]"}]}], "}"}], ",", 
           RowBox[{"FuseDmatrices", "[", 
            RowBox[{
             RowBox[{"Atree", "[", 
              RowBox[{"1", ",", 
               RowBox[{"Zs", "[", 
                RowBox[{"[", 
                 RowBox[{"{", 
                  RowBox[{"1", ",", "m", ",", 
                   RowBox[{"m", "+", "1"}], ",", 
                   RowBox[{"-", "2"}], ",", 
                   RowBox[{"-", "1"}]}], "}"}], "]"}], "]"}]}], "]"}], 
             RowBox[{"Atree", "[", 
              RowBox[{"kL", ",", 
               RowBox[{"Append", "[", 
                RowBox[{
                 RowBox[{"Zs", "[", 
                  RowBox[{"[", 
                   RowBox[{"1", ";;", "m"}], "]"}], "]"}], ",", "Zmm"}], 
                "]"}]}], "]"}], 
             RowBox[{"Atree", "[", 
              RowBox[{
               RowBox[{"k", "-", "1", "-", "kL"}], ",", 
               RowBox[{"Prepend", "[", 
                RowBox[{
                 RowBox[{"Append", "[", 
                  RowBox[{
                   RowBox[{"Zs", "[", 
                    RowBox[{"[", 
                    RowBox[{
                    RowBox[{"m", "+", "1"}], ";;", 
                    RowBox[{"-", "2"}]}], "]"}], "]"}], ",", 
                   RowBox[{"inter", "[", 
                    RowBox[{"Zs", "[", 
                    RowBox[{"[", 
                    RowBox[{"{", 
                    RowBox[{
                    RowBox[{"-", "1"}], ",", 
                    RowBox[{"-", "2"}], ",", "1", ",", "m", ",", 
                    RowBox[{"m", "+", "1"}]}], "}"}], "]"}], "]"}], "]"}]}], 
                  "]"}], ",", "Zmm"}], "]"}]}], "]"}]}], "]"}]}], "]"}], ",", 
         
         RowBox[{"{", 
          RowBox[{"m", ",", "2", ",", 
           RowBox[{
            RowBox[{"Length", "[", "Zs", "]"}], "-", "3"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"kL", ",", "0", ",", 
           RowBox[{"k", "-", "1"}]}], "}"}]}], "]"}]}]}], "]"}]}], 
   "]"}]}]}], "Code",
