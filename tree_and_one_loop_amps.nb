Cell[BoxData[{
 RowBox[{
  RowBox[{"fourbracket", "[", "Zs_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "Z", "}"}], ",", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"Distribute", "[", 
       RowBox[{"ab", "@@", 
        RowBox[{"(", 
         RowBox[{"Zs", ".", 
          RowBox[{"Array", "[", 
           RowBox[{"Z", ",", 
            RowBox[{"Length", "[", 
             RowBox[{"Zs", "[", 
              RowBox[{"[", "1", "]"}], "]"}], "]"}]}], "]"}]}], ")"}]}], 
       "]"}], "/.", "\n", " ", 
      RowBox[{
       RowBox[{"ab", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"i1_:", "1"}], ")"}], 
          RowBox[{"Z", "[", "j1_", "]"}]}], ",", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"i2_:", "1"}], ")"}], 
          RowBox[{"Z", "[", "j2_", "]"}]}], ",", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"i3_:", "1"}], ")"}], 
          RowBox[{"Z", "[", "j3_", "]"}]}], ",", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"i4_:", "1"}], ")"}], 
          RowBox[{"Z", "[", "j4_", "]"}]}]}], "]"}], "\[RuleDelayed]", 
       RowBox[{"i1", " ", "i2", " ", "i3", " ", "i4", " ", 
        RowBox[{"ab", "[", 
         RowBox[{"j1", ",", "j2", ",", "j3", ",", "j4"}], "]"}]}]}]}], 
     ")"}]}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{"inter", "[", 
   RowBox[{"{", 
    RowBox[{"i1_", ",", "i2_", ",", "j1_", ",", "j2_", ",", "j3_"}], "}"}], 
   "]"}], ":=", 
  RowBox[{
   RowBox[{"i1", " ", 
    RowBox[{"fourbracket", "[", 
     RowBox[{"{", 
      RowBox[{"i2", ",", "j1", ",", "j2", ",", "j3"}], "}"}], "]"}]}], "-", 
   RowBox[{"i2", " ", 
    RowBox[{"fourbracket", "[", 
     RowBox[{"{", 
      RowBox[{"i1", ",", "j1", ",", "j2", ",", "j3"}], "}"}], 
     "]"}]}]}]}]}], "Code",
